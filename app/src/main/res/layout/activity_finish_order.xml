<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FinishOrderActivity">

    <Button
        android:id="@+id/backBtn"
        android:layout_width="56dp"
        android:layout_height="46dp"
        app:iconTint="@color/text"
        app:layout_constraintBottom_toBottomOf="@+id/textEndOrder"
        android:layout_marginStart="15dp"
        android:background="@android:color/transparent"
        app:icon="@drawable/backbtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textEndOrder" />

    <TextView
        android:id="@+id/textEndOrder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="49dp"
        android:fontFamily="@font/candarabold"
        android:text="@string/endorder"
        android:textAllCaps="true"
        android:textAppearance="@style/NamePages"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/orderNumb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:fontFamily="@font/candarabold"
        android:text="@string/endorderend"
        android:textAllCaps="true"
        android:textColor="@color/text"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/checkedImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orderNumb"
        app:srcCompat="@drawable/checked" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="550dp"
        android:layout_height="250dp"
        android:background="@drawable/retangle_complete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageItemView"
        android:layout_width="138dp"
        android:layout_height="138dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginBottom="-55dp"/>

    <TextView
        android:id="@+id/finalizedOrderBottomName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/trebuc"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="24dp"
        app:layout_constraintEnd_toEndOf="@+id/imageItemView"
        app:layout_constraintStart_toStartOf="@+id/imageItemView"
        app:layout_constraintTop_toBottomOf="@+id/imageItemView" />

    <ImageView
        android:id="@+id/view7"
        android:layout_width="245dp"
        android:layout_height="44dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/background_ratingbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/finalizedOrderBottomName" />

    <RatingBar
        android:id="@+id/ratingBar_itensOrdem"
        style="?android:attr/ratingBarStyleIndicator"
        android:layout_width="183dp"
        android:layout_height="32dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        android:isIndicator="false"
        android:max="5"
        android:scaleX="0.95"
        android:scaleY="0.95"
        android:theme="@style/MyRatingBar"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <TextView
        android:id="@+id/menu_foodEvauation3"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/segoeuil"
        android:gravity="center_vertical"
        android:text="5,00"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="@+id/textView70"
        app:layout_constraintStart_toEndOf="@+id/textView70"
        app:layout_constraintTop_toTopOf="@+id/textView70" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/segoeuisl"
        android:text="@string/order_num"
        android:textAllCaps="true"
        android:textColor="@color/text"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/orderNumb"
        app:layout_constraintStart_toStartOf="@+id/orderNumb" />

    <TextView
        android:id="@+id/evaluatebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="-2dp"
        android:fontFamily="@font/candarabold"
        android:padding="10dp"
        android:text="OK"
        android:textAllCaps="true"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7" />

    <TextView
        android:id="@+id/textView70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/segoeuil"
        android:text="Avaliação: "
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

</androidx.constraintlayout.widget.ConstraintLayout>